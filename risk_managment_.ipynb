    "import pdfplumber\n",
    "\n",
    "def extract_text_from_pdf(pdf_file_path, start_page, end_page):\n",
    "    try:\n",
    "        with pdfplumber.open(pdf_file_path) as pdf:\n",
    "            text = \"\"\n",
    "\n",
    "            for page_number in range(start_page - 1, end_page):\n",
    "                page = pdf.pages[page_number]\n",
    "                page_text = page.extract_text()\n",
    "                text += page_text\n",
    "\n",
    "            return text\n",
    "\n",
    "    except Exception as e:\n",
    "        print(f\"An error occurred: {str(e)}\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    pdf_file_path = \"PMBOK6-2017.pdf\"\n",
    "    start_page = 431\n",
    "    end_page = 494\n",
    "    extracted_text = extract_text_from_pdf(pdf_file_path, start_page, end_page)\n",
    "\n",
    "    if extracted_text:\n",
    "        print(extracted_text)\n",
    "    else:\n",
    "        print(\"No text found in the specified pages of the PDF.\")\n"
   ]
