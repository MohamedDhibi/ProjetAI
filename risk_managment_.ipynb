    "import fitz  # PyMuPDF\n",
    "import re\n",
    "\n",
    "def find_figures_and_tables(pdf_path):\n",
    "    doc = fitz.open(pdf_path)\n",
    "    figures = []\n",
    "    tables = []\n",
    "\n",
    "    for page_num in range(len(doc)):\n",
    "        page = doc[page_num]\n",
    "        text = page.get_text()\n",
    "\n",
    "        # Detect figures based on keywords like \"Figure\" or \"Fig.\"\n",
    "        figure_matches = re.finditer(r'(?:Figure|Fig\\.)\\s+\\d+', text, re.IGNORECASE)\n",
    "        for match in figure_matches:\n",
    "            figure_text = match.group(0)\n",
    "            figures.append({\n",
    "                'Page': page_num + 1,\n",
    "                'Text': figure_text,\n",
    "            })\n",
    "\n",
    "        # Detect tables based on keywords like \"Table\"\n",
    "        table_matches = re.finditer(r'Table\\s+\\d+', text, re.IGNORECASE)\n",
    "        for match in table_matches:\n",
    "            table_text = match.group(0)\n",
    "            tables.append({\n",
    "                'Page': page_num + 1,\n",
    "                'Text': table_text,\n",
    "            })\n",
    "\n",
    "    doc.close()\n",
    "\n",
    "    return figures, tables\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    pdf_path = 'your_pdf_file.pdf'  # Replace with your PDF file path\n",
    "    figures, tables = find_figures_and_tables(pdf_path)\n",
    "\n",
    "    print(\"Figures:\")\n",
    "    for figure in figures:\n",
    "        print(f\"Page {figure['Page']}: {figure['Text']}\")\n",
    "\n",
    "    print(\"\\nTables:\")\n",
    "    for table in tables:\n",
    "        print(f\"Page {table['Page']}: {table['Text']}\")\n"
