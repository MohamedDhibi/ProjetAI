        "\n",
        "def count_figures_and_tables_in_range(pdf_file_path, start_page, end_page):\n",
        "    figure_count = 0\n",
        "    table_count = 0\n",
        "\n",
        "    with pdfplumber.open(pdf_file_path) as pdf:\n",
        "        for page_num, page in enumerate(pdf.pages, start=1):\n",
        "            if page_num < start_page:\n",
        "                continue  # Skip pages before the start page\n",
        "\n",
        "            if page_num > end_page:\n",
        "                break  # Stop when reaching the end page\n",
        "\n",
        "            page_text = page.extract_text()\n",
        "\n",
        "            # Count figures based on specific keywords or patterns\n",
        "            if 'Figure' in page_text:\n",
        "                figure_count += page_text.count('Figure')\n",
        "                print(f\"Figure(s) found on Page {page_num}\")\n",
        "\n",
        "            # Count tables based on specific keywords or patterns\n",
        "            if 'Table' in page_text:\n",
        "                table_count += page_text.count('Table')\n",
        "                print(f\"Table(s) found on Page {page_num}\")\n",
        "\n",
        "    return figure_count, table_count\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    pdf_file_path = \"PMBOK6-2017.pdf\"  # Replace with the path to your PDF file\n",
        "    start_page = 431\n",
        "    end_page = 494\n",
        "    figure_count, table_count = count_figures_and_tables_in_range(pdf_file_path, start_page, end_page)\n",
        "\n",
        "    print(f\"Total Figures: {figure_count}\")\n",
        "    print(f\"Total Tables: {table_count}\")\n"
      ],
