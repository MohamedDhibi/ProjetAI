        "from io import StringIO\n",
        "from pdfminer.converter import TextConverter\n",
        "from pdfminer.layout import LAParams\n",
        "from pdfminer.pdfinterp import PDFPageInterpreter\n",
        "from pdfminer.pdfinterp import PDFResourceManager\n",
        "from pdfminer.pdfpage import PDFPage\n",
        "\n",
        "PMBOK6 = \"PMBOK6-2017.pdf\"\n",
        "\n",
        "Script = []\n",
        "start_page = 431\n",
        "end_page = 494\n",
        "\n",
        "with open(PMBOK6, 'rb') as pdf_file:\n",
        "    resource_manager = PDFResourceManager()\n",
        "    text_io = StringIO()\n",
        "    laparams = LAParams()\n",
        "    converter = TextConverter(resource_manager, text_io, laparams=laparams)\n",
        "    interpreter = PDFPageInterpreter(resource_manager, converter)\n",
        "\n",
        "    for page_num, page in enumerate(PDFPage.get_pages(pdf_file), start=1):\n",
        "        if start_page <= page_num <= end_page:\n",
        "            interpreter.process_page(page)\n",
        "\n",
        "    text = text_io.getvalue()\n",
        "    Script.append(text)\n",
        "    print(text)\n",
        "\n",
        "with open(\"extracted_text.txt\", \"w\", encoding=\"utf-8\") as output_file:\n",
        "    output_file.write(text)\n"
      ]
